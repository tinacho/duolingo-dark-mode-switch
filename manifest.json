{
  "manifest_version": 3,
  "name": "Duolingo Dark Mode",
  "description": "Enable native dark mode on Duolingo website",
  "version": "1.0",
  "icons": {
    "128": "images/icon-128.png"
  },
  "action": {
    "default_icon": {
      "128": "images/icon-128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.duolingo.com/*"
      ],
      "js": [
        "script.js"
      ]}
  ]
}
